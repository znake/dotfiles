# to make file listings more readable with color
export CLICOLOR=1
export PATH=${PATH}:/Users/jakoblehner/bin/android-sdk/tools
export PATH=${PATH}:/Users/jakoblehner/bin/android-sdk/platform-tools
PATH=$PATH:/usr/local/zend/bin:/usr/local/zend/mysql/bin
    LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/local/zend/lib

# using wayneeseguin's bash-prompt within .rvm/contrib
if [ -d ~/.rvm ]; then 
  ps1_set --prompt $ 
else 
  echo "please install rvm -> https://rvm.beginrescueend.com/rvm/install/"
fi

